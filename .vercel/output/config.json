{
	"version": 3,
	"routes": [
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"handle": "filesystem"
		},
		{
			"src": "^\\/_image$",
			"dest": "_render"
		},
		{
			"src": "^\\/address\\/([^/]+?)\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/api\\/ethscriptions\\/([^/]+?)\\/content\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/api\\/ethscriptions\\/([^/]+?)\\/creator\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/api\\/ethscriptions\\/([^/]+?)\\/number\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/api\\/ethscriptions\\/([^/]+?)\\/owner\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/api\\/ethscriptions\\/([^/]+?)\\/transfers\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/api\\/ethscriptions\\/([^/]+?)\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/api\\/ethscriptions\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/api\\/exists\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/api\\/resolve\\/([^/]+?)\\/created\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/api\\/resolve\\/([^/]+?)\\/owned\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/api\\/resolve\\/([^/]+?)\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/api\\/search\\/([^/]+?)\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/api\\/txs\\/([^/]+?)\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/pages\\/404\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/tx\\/([^/]+?)\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/([^/]+?)\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/$",
			"dest": "_render"
		}
	]
}